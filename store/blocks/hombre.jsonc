    "info-card#col-shirt-hombre-right",
    "info-card#col-shirt-hombre-rightM"
  ]
},
"info-card#col-shirt-hombre-right":{
  "props": {
    "id": "info-card-shirt",
    "isFullModeStyle": true,
    "fullWidth": true,
    //"textPosition": "left",
    "imageUrl": "assets/desk/hombre/banner-vitrina2.jpg",
    "callToActioMode": "none",
    //"headline": "Clearance Sale",
    //"callToActionText": "DISCOVER",
    //"callToActionUrl": "/sale/d",
    "blockClass": ["info-card-shirt-right", "depto-desk"]
  }
},
"info-card#col-shirt-hombre-rightM":{
  "props": {
    "id": "info-card-shoes",
    "isFullModeStyle": true,
    "fullWidth": true,
    "imageUrl": "https://callearturop.vtexassets.com/arquivos/dep-hombre-banner-vitrina2.jpg",
    "callToActioMode": "none",
    "blockClass": ["info-card-shoes-left", "depto-mobile"]
  }
},
"shelf#shirt-hombre": {
  "blocks": ["product-summary.shelf"],
  "props": {
    "blockClass": "shirt",
    "orderBy":"OrderByTopSaleDESC",
    "paginationDotsVisibility": "desktopOnly",
    "productList":{
      "maxItems": 10,
      "itemsPerPage": 3,
      "minItemsPerPage": 2,
      "scroll": "BY_PAGE",
      "arrows": true,
      "showTitle": false
    }
  }
},

