{
  "store.custom#nina": {
  "blocks": [
    "info-card#nina-home",
    "info-card#nina-home-Mobile",
    //"list-context.image-list#demo",
    /* You can make references to blocks defined in other files.
     * For example, `flex-layout.row#deals` is defined in the `deals.json` file. */
    "__fold__.experimentalLazyAssets",
    "flex-layout.row#dealsNina",
    "slider-layout#nina-deals-mobile",
    "rich-text#shelf-titleDepto",
    "flex-layout.row#shoes-nina",
    "flex-layout.row#shirt-nina",
    "flex-layout.row#nina-banner",
    "flex-layout.row#nina-bannerM",

    "flex-layout.row#product__tabs-nina",


    "flex-layout.row#nina-banner2",
    "flex-layout.row#nina-banner2M",
    "shelf#descubre-mas-nina",
    "shelf#lo-mas-vendido-nina"
  ]
},

"list-context.image-list#demo-nina": {
  "children": ["slider-layout#demo-images-nina"],
  "props": {
    "height": 720,
    "images": [
      {
        "image": "https://storecomponents.vteximg.com.br/arquivos/banner-principal.png",
        "mobileImage": "https://storecomponents.vteximg.com.br/arquivos/banner-principal-mobile.jpg"
      },
      {
        "image": "https://storecomponents.vteximg.com.br/arquivos/banner.jpg",
        "mobileImage": "https://storecomponents.vteximg.com.br/arquivos/banner-principal-mobile.jpg"
      }
    ]
  }
},
"slider-layout#demo-images-nina": {
  "props": {
    "itemsPerPage": {
      "desktop": 1,
      "tablet": 1,
      "phone": 1
    },
    "infinite": true,
    "showNavigationArrows": "desktopOnly",
    "blockClass": "carousel"
  }
},

/*--------------------------------------------------------*/
/* Banner vitrina 1 --> CONOCE NUESTRA LINEA DE ZAPATOS  */
/*------------------------------------------------------*/
"rich-text#shelf-title": {
  "props": {
    "text": "CONOCE NUESTRA LINEA DE ZAPATOS",
    "blockClass": "shelfTitle"
  }
},
"flex-layout.row#shoes-nina":{
  "props": {
    "blockClass": ["more","vitrina1"]
  },
  "children": [
    "flex-layout.col#shoes-nina-left",
    "flex-layout.col#shoes-nina-right"]
},
"flex-layout.col#shoes-nina-left": {
  "props": {
    "width": "25%",
    "blockClass": ["banner-vitrina2","vitrina2-1"]
  },
  "children": [
    "info-card#col-shoes-nina-left",
    "info-card#col-shoes-nina-leftM"
  ]
},
"flex-layout.col#shoes-nina-right": {
  "props": {
    "width": "75%"
  },
  "children": [
    "shelf#shoes-nina"
  ]
},
"info-card#col-shoes-nina-left":{
  "props": {
    "id": "info-card-shoes",
    "isFullModeStyle": true,
    "fullWidth": true,
    //"textPosition": "left",
    "imageUrl": "assets/desk/nina/banner-vitrina1.jpg",
    "callToActioMode": "none",
    "blockClass": ["info-card-shoes-left", "depto-desk"]
    //"textAlignment": "center"
  }
},
"shelf#shoes-nina": {
  "blocks": ["product-summary.shelf"],
  "props": {
    "blockClass": "shoes",
    "orderBy":"OrderByTopSaleDESC",
    "paginationDotsVisibility": "desktopOnly",
    "collection": 140,
    "productList":{
      "maxItems": 10,
      "itemsPerPage": 3,
      "minItemsPerPage": 2,
      "scroll": "BY_PAGE",
      "arrows": true,
      "showTitle": false
    }
  }
},


