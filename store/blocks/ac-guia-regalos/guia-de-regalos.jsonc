  },
  "slider-layout#demo-category-2M": {
      "props": {
        "itemsPerPage": {
          "desktop": 3,
          "tablet": 3,
          "phone": 1
        },
        "infinite": true,
        "fullWidth": false,
        "blockClass": "shelf",
        "showPaginationDots":"false",
        "showNavigationArrows": "never"
      }
  }
