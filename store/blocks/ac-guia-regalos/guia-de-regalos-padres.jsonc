      "search-result-layout.customQuery#guia-regalos-padres"
    ]
  },
  "image#freedom": {
    "props": {
      "minWidth": "100%",
      "src": "https://arturocalle.vteximg.com.br/arquivos/Banner-Categoria-Freedom.jpg"
    }
  },
  "search-result-layout.customQuery#guia-regalos-padres": {
    "props": {
      "querySchema": {
        "orderByField": "OrderByReleaseDateDESC",
        "hideUnavailableItems": true,
        "maxItemsPerPage": 8,
        "queryField": "751",
        "mapField": "productClusterIds"
      }
    },
    "blocks": [
      "search-result-layout.desktop#customC",
      "search-result-layout.mobile#customC",
      "search-not-found-layout"
    ]
  },
  "search-result-layout.desktop#customC": {
    "children": [
      //"flex-layout.row#vitrinaDestacadaSearch",
      "flex-layout.row#searchbread",
      "flex-layout.row#searchtitle",
      "flex-layout.row#result"
    ],
    "props": {
      "pagination": "show-more",
      "preventRouteChange": false,
      "defaultGalleryLayout": "grid",
      "maxItemsPerPage": 12
    }
  },
  "search-result-layout.mobile#customC": {
    "children": [
      "flex-layout.row#searchinfoM",
      //"flex-layout.row#fetchprevious",
      "flex-layout.row#products",
      "flex-layout.row#fetchmore"
    ],
    "props": {
      "pagination": "show-more",
      "mobileLayout": {
        "mode1": "small",
        "mode2": "normal"
      },
      "defaultGalleryLayout": "grid"
    }
  },


