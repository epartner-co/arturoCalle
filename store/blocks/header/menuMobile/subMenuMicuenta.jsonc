    "submenu.accordion#cuenta-principal":{
        "children":[
            "menu#cuenta-principal"
        ],
        "props":{
            "blockClass":"menuGeneral"
        }
    },
    // agrupacion de los items
    "menu#cuenta-principal":{
        "children":[
            "menu-item#category-cuenta-pedido-M",
            "menu-item#category-cuenta-cuenta-M"
        ],
        "props":{
            "orientation": "vertical",
            "blockClass":"menuGeneral-sub"
        }
        
    },
    // items principales del sub menu
    "menu-item#category-cuenta-pedido-M": {
        "props": {
          "id": "menu-item-category-pedido",
